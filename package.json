{
  "name": "@jeffcaradona/eslint-plugin-eta",
  "version": "0.1.1",
  "description": "An ESLint plugin so you can lint Eta template files",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "test:watch": "jest --coverage --watch",
    "format": "prettier --write lib/**/*.js test/**/*.js",
    "lint": "eslint lib/**/*.js test/**/*.js --ext .js",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "author": "jeffcaradona",
  "contributors": [
    "shadowtime2000 (Original Author)"
  ],
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=8 <9",
    "eta": ">=1.0.0 <5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.11.0",
    "eslint-config-eta-dev": "^1.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eta": "^1.11.0",
    "jest": "^26.6.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  }
}
